<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <Button
        android:id="@+id/bt_video"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="跳转到自定义播放器" />

    <Button
        android:id="@+id/bt_publish"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="跳转到推流" />

    <Button
        android:id="@+id/bt_net"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="请求下接口，打点，登陆判断" />

    <Button
        android:id="@+id/bt_recyclerview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="跳转到recycler" />

    <Button
        android:id="@+id/bt_chatroom"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="跳转到聊天室" />

    <Button
        android:id="@+id/bt_flex"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="跳转到flex" />

    <Button
        android:id="@+id/bt_mediacodec"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="跳转到mediacodec" />

    <Button
        android:id="@+id/bt_tomcat"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="跳转到TOM猫" />

    <Button
        android:id="@+id/bt_changevoice"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="正儿八经的变声" />

    <Button
        android:id="@+id/bt_gl"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="跳转到gl" />

    <Button
        android:id="@+id/bt_supervideo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="跳转到差不多得了" />

</LinearLayout>